{
  "multipart": [
    {
      "apply": {
        "model": "quark:block/gold_bars_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "quark:block/gold_bars_side"
      }
    },
    {
      "when": {
        "north": "false"
      },
      "apply": {
        "model": "quark:block/gold_bars_noside"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "quark:block/gold_bars_side_alt"
      }
    },
    {
      "when": {
        "south": "false"
      },
      "apply": {
        "model": "quark:block/gold_bars_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "quark:block/gold_bars_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": "false"
      },
      "apply": {
        "model": "quark:block/gold_bars_noside",
        "y": 270
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "quark:block/gold_bars_side",
        "y": 90
      }
    },
    {
      "when": {
        "east": "false"
      },
      "apply": {
        "model": "quark:block/gold_bars_noside_alt"
      }
    }
  ]
}